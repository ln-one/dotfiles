# ========================================
# Starship Prompt Configuration
# ========================================
# Modern configuration optimized for {{ .environment }} environment

{{- if .features.enable_starship }}

# Use Starship default configuration with minimal customization
add_newline = false

# Disable Node.js version display
[nodejs]
disabled = true

# Disable Python version display
[python]
disabled = true

{{- else }}
# Starship feature disabled
{{- end }}